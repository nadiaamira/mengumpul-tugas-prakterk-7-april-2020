#include<iostream>
using namespace std;
int main(){
	//tugas nadia amira putri
	//membuat program goto
	char jump = 'C' ;
	cout<< "nama: nadia amira putri" << endl;
	cout<< "nim: ETE10190111"<< endl;
	cout<< "Masukkan huruf a untuk melompat" << endl;
	cout<< "Masukkan huruf b untuk tidak melompat" << endl;
	cin>> jump;
	
	cout << "X" <<endl;
	if ( jump == 'a' ) goto lompat;
	cout << "Y" <<endl;
	lompat:
	cout << "Z" <<endl;
	
	return 0;
}
